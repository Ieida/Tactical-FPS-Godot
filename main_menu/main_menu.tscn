resource_name = "main_menu"
script/source = "extends Control

func _ready():
	$MainMenu.show()
	$SettingsMenu.hide()
	
	# Button signals
	$MainMenu/Play.pressed.connect(_on_main_menu_play_pressed)
	$MainMenu/Settings.pressed.connect(_on_main_menu_settings_pressed)
	$MainMenu/Quit.pressed.connect(_on_main_menu_quit_pressed)
	
	$SettingsMenu/Back.pressed.connect(_on_settings_menu_back_pressed)

func _on_main_menu_play_pressed():
	print(\"MAIN MENU PLAY NOT IMPLEMENTED\")

func _on_main_menu_settings_pressed():
	$MainMenu.hide()
	$SettingsMenu.show()

func _on_main_menu_quit_pressed():
	get_tree().quit(0)

func _on_settings_menu_back_pressed():
	$SettingsMenu.hide()
	$MainMenu.show()
"

