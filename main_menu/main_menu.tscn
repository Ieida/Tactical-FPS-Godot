resource_name = "sensitivity_slider"
script/source = "extends HSlider

@export var sens: Sensitivity

func _ready():
	set_value_no_signal(sens.value)
	$Label2.text = str(sens.value)
	value_changed.connect(_on_value_changed)

func _on_value_changed(new_value: float):
	$Label2.text = str(new_value)
	sens.value = new_value
"

