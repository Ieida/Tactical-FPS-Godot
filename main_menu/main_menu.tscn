script/source = "extends GameMenuButton

var path_string = \"res://level_0/level_0.tscn\"

func _ready():
	pressed.connect(_on_pressed)
	ResourceLoader.load_threaded_request(path_string)

func _on_pressed():
	var scene = ResourceLoader.load_threaded_get(path_string)
	get_tree().change_scene_to_packed(scene)
